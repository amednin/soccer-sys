{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Match</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Resultteam2</th>
                <td>{{ entity.resultTeam2 }}</td>
            </tr>
            <tr>
                <th>Resultteam1</th>
                <td>{{ entity.resultTeam1 }}</td>
            </tr>
            <tr>
                <th>Matchdate</th>
                <td>{{ entity.matchDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Matchtime</th>
                <td>{{ entity.matchTime }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{{ entity.updatedAt|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('fixture') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('fixture_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
